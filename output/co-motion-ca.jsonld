    "@id": "https://co-motion.ca/spectacles/marc-dupre-nouveau-spectacle/",
    "http://ogp.me/ns#description": [
      {
        "@language": "fr",
        "@value": "Après une pause bien méritée, Marc Dupré est de retour avec un tout nouveau spectacle qui s'annonce électrisant « Parce que ce soir »!"
      }
    ],
    "http://ogp.me/ns#image": [
